{
  "name": "printer",
  "version": "1.0.0",
  "description": "Prints all the stuff on the pages, converts various e-book formats, structurize the book via parts and chapters and all other typographer work",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --skipLibCheck --project . && cp src/config/config.json dist/config",
    "test": "ava-ts",
    "test:watch": "npm test -- --watch",
    "proc": "NODE_PATH=. pm2 start --no-daemon",
    "start": "pm2 start --no-daemon",
    "start:dev": "nodemon",
    "lint": "tslint --project .",
    "gen:types": "graphql-schema-typescript generate-ts src/schema/schema.graphql --output src/types/schema.ts --typePrefix="
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hyped-text/printer.git"
  },
  "keywords": [
    "html",
    "parser",
    "converter",
    "composite"
  ],
  "author": "shonie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyped-text/printer/issues"
  },
  "homepage": "https://github.com/hyped-text/printer#readme",
  "dependencies": {
    "@ridi/epub-parser": "^0.5.2",
    "apollo-server-koa": "^2.4.8",
    "apollo-server-redis": "0.1.0-rc.11",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.1.0",
    "dotenv-cli": "^1.4.0",
    "epub": "^1.1.0",
    "graphql": "^14.2.1",
    "graphql-api-koa": "^2.0.0",
    "graphql-tag": "^2.10.1",
    "graphql-upload": "^8.0.4",
    "hyped-logger": "git+https://github.com/hyped-text/hyped-logger.git",
    "is-absolute-url": "^2.1.0",
    "is-relative-url": "^2.0.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-logger-winston": "0.0.2",
    "koa-router": "^7.4.0",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "nconf": "^0.10.0",
    "node-fetch": "^2.3.0",
    "parse-database-url": "^0.3.0",
    "pm2": "^3.2.4",
    "ramda": "^0.26.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/dotenv": "^6.1.0",
    "@types/epub": "0.0.5",
    "@types/graphql": "^14.2.0",
    "@types/graphql-api-koa": "^2.0.0",
    "@types/is-absolute-url": "^2.1.0",
    "@types/is-relative-url": "^2.0.0",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-router": "^7.0.40",
    "@types/lowdb": "^1.0.7",
    "@types/mkdirp": "^0.5.2",
    "@types/nconf": "^0.10.0",
    "@types/node": "^10.14.4",
    "@types/node-fetch": "^2.1.7",
    "@types/ramda": "^0.25.48",
    "@types/sinon": "^7.0.5",
    "@types/uuid": "^3.4.4",
    "ava": "^1.2.1",
    "ava-ts": "^0.25.2",
    "graphql-schema-typescript": "^1.2.9",
    "nodemon": "^1.18.10",
    "prettier": "^1.15.3",
    "sinon": "^7.2.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.4.1"
  }
}
